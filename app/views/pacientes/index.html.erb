<p id="notice"><%= notice %></p>

<h1>Pacientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Data nasc</th>
      <th>Sexo</th>
      <th>Email</th>
      <th>Logradouro</th>
      <th>Numero</th>
      <th>Bairro</th>
      <th>Cep</th>
      <th>Cep</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th>Pais</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.nome %></td>
        <td><%= paciente.cpf %></td>
        <td><%= paciente.data_nasc %></td>
        <td><%= paciente.sexo %></td>
        <td><%= paciente.email %></td>
        <td><%= paciente.logradouro %></td>
        <td><%= paciente.numero %></td>
        <td><%= paciente.bairro %></td>
        <td><%= paciente.cep %></td>
        <td><%= paciente.cep %></td>
        <td><%= paciente.cidade %></td>
        <td><%= paciente.estado %></td>
        <td><%= paciente.pais %></td>
        <td><%= link_to 'Show', paciente %></td>
        <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
        <td><%= link_to 'Destroy', paciente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paciente', new_paciente_path %>
